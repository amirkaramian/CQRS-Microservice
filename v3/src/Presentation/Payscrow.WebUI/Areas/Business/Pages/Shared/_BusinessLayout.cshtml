                                        </svg>
                                    </span>
                                    <!--end::Svg Icon-->
                                </div>
                                <!--end::Aside mobile toggle-->
                                <!--begin::Logo-->
                                <a asp-page="/Index" asp-area="Business" class="d-flex align-items-center">
                                    <img alt="Logo" src="/images/logo-business.png" class="h-20px" />
                                </a>
                                <!--end::Logo-->
                            </div>
                            <!--end::Wrapper-->
                            <!--begin::Topbar-->
                            <div class="d-flex align-items-center flex-shrink-0">
                                <!--begin::Search-->
                                <div class="d-flex align-items-center w-125px w-md-150px w-lg-225px" data-kt-search-keypress="true" data-kt-search-min-length="2" data-kt-search-enter="enter" data-kt-search-layout="menu" data-kt-menu-trigger="auto" data-kt-menu-permanent="true" data-kt-menu-placement="bottom-end">
